{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "ignore": [
      "__pycache__",
      "*.pyc",
      "*.pyo",
      "env",
      "venv",
      ".idea",
      ".vscode",
      "results",
      ".DS_Store"
    ]
  }
  